  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\SampleBatch.Common\SampleBatch.Common.csproj" />
    <ProjectReference Include="..\SampleBatch.Components\SampleBatch.Components.csproj" />
  </ItemGroup>

